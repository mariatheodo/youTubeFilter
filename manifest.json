{
  "name": "YouTube Filter",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "My first Chrome extension.",
  "homepage_url": "http://www.youtube.com",
  "page_action": {
    "default_icon": "img/favicon-vflz7uhzw.ico"
  },
  "content_scripts": [
    {
    	"matches": ["https://*.youtube.com/*"],
      "js": ["scripts/myscript.js"],
      "run_at":"document_start"
    }],
  "permissions": [
      "tabs"
  ]
}